// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model City {
  id         Int       @id @default(autoincrement())
  name       String
  updatedAt  DateTime  @updatedAt

  temp       Float
  desc       String
  speed      Float
  deg        Int
  feelsLike  Float
  tempMax    Float
  tempMin    Float
  pressure   Int
  humidity   Int
}

